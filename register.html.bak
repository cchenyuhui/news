  <div class="container container-small">
	<h1>注册</h1>
	<form>
		<div class="form-group">
			<div class="username">
			<label>用户名</label>
				<input type="text" class="form-control name" >
				<div class="nameerror"></div>
			</div >
			<div class="password">
			<label>密码</label>
				<input type="text" class="form-control word">
			</div>
			<div class="phone">
			<label>绑定手机号</label>
				<input type="text" class="form-control number">
			</div>
			<div class="identifying code">
			<label>验证码</label>
				<div class="input-group">
					
					<input type="text" class="form-control identify">
					<div class="input-group-btn">
						<div class="btn btn-default">获取验证码</div>
					</div>
					
				</div>
			</div>
		
			<button class="btn-primary btn btn-block" type="submit">提交</button>
		</div>
	</form>
